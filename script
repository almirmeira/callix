sudo dpkg -i lmbench_3.0-a9-1.3+b1_amd64.deb
wget https://raw.githubusercontent.com/sivel/speedtest-cli/master/speedtest_cli.py
apt -y install nuttcp
sudo apt -y install -t jessie-backports conky conky-all 
sudo apt -y install conky conky-all lua5.1 curl lm-sensors lsb-release
sudo apt -y install lua5.1 curl lm-sensors lsb-release
sudo apt -y install hping3
sudo apt -y install -y pkg-config dh-autoreconf ncurses-dev build-essential libssl-dev libp$
sudo apt -y install git
